    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.21.5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "css-loader": "^6.7.4",
    "dotenv": "^16.0.3",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "hugo-extended": "^0.111.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-cli": "^10.1.0",
